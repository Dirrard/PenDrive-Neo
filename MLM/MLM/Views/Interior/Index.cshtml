@using MLM.Models
<div class="row">
    <div class="col-md-12">
        <br />
        <a href="@Url.Action("Cadastro")" class="btn btn-primary">Nova Interior</a>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="table-responsive">
            <br />
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>Nome</th>
                        <th>Ar condicionado</th>
                        <th>Rádio</th>
                        <th>Volante</th>
                        <th>Preco</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (Interior item in Model)
                    {
                        <tr>
                            <td>@item.Banco</td>
                            <td>@item.Ac</td>
                            <td>@item.Radio</td>
                            <td>@item.Volante</td>
                            <td>@item.Preco</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
